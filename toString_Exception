public class tostring_Exception {
    public static void main(String[] args) {
        int a=10,b=0,c;
        try{
            c=a/b;
            System.out.println(c);
        }
        catch(ArithmeticException e){
            System.out.println(e);               //Both will print same message
            System.out.println(e.toString());
        }
        System.out.println("Program is finshed!");
    }
}
 
/**  toString method prints
 1.Exception name 
 2.Description  **/
